  "dependencies": {
    "@types/node": "^20.10.6",
    "buffer": "^6.0.3"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "acorn-walk": "^8.3.0",
    "esbuild": "^0.19.11",
    "eslint": "^8.54.0",
    "eslint-config-xo": "^0.43.1",
    "eslint-config-xo-typescript": "^1.0.1",
    "playwright": "^1.40.0",
    "typescript": "^5.3.2"
  }
}
